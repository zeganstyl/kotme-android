<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/completed">
        <shape
            android:shape="oval" >

            <stroke
                android:width="3dp"
                android:color="#5DB725" />

            <solid
                android:color="#705DB725"
                />
        </shape>
    </item>

    <item android:id="@+id/current">
        <shape
            android:shape="oval" >

            <stroke
                android:width="3dp"
                android:color="#ECF00D" />

            <solid
                android:color="#70ECF00D"
                />
        </shape>
    </item>

    <item android:id="@+id/undiscovered">
        <clip>
            <shape
                android:shape="oval" >
            </shape>
        </clip>
    </item>

</layer-list>